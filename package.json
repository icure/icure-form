{
	"name": "@icure/form",
	"version": "3.1.0",
	"description": "",
	"main": "lib/index.js",
	"scripts": {
		"start": "rspack serve --mode development",
		"build": "node lit-inject-scss.js src tmp && jq '{include:[\"tmp\"], compilerOptions:.compilerOptions}' < tsconfig.json > tsconfig.tmp.json && tsc -p tsconfig.tmp.json",
		"dist": "rspack build --mode production",
		"prepare": "yarn run build && jq '{name:.name, version:.version, description:.description, main:.main, types:.types, dependencies:.dependencies, author:.author, license:.license, bugs:.bugs, homepage:.homepage}' < package.json > lib/package.json",
		"publish": "npm login && yarn run prepare && cd lib && touch yarn.lock && yarn install && rm -rf node_modules && npm publish --tolerate-republish --access public && cd ..",
		"test": "jest",
		"lint:commit": "eslint . --ext .ts",
		"pre-commit": "lint-staged"
	},
	"keywords": [
		"litElement",
		"ProseMirror",
		"node",
		"typescript"
	],
	"jest": {
		"preset": "ts-jest",
		"testEnvironment": "node",
		"testRegex": "(/test/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
		"moduleFileExtensions": [
			"ts",
			"tsx",
			"js",
			"jsx",
			"json",
			"node",
			"yaml"
		],
		"transform": {
			"^.+\\.tsx?$": [
				"ts-jest",
				{
					"tsconfig": {
						"esModuleInterop": true,
						"allowSyntheticDefaultImports": true
					}
				}
			]
		}
	},
	"husky": {
		"hooks": {
			"pre-commit": "npm run pre-commit"
		}
	},
	"lint-staged": {
		"*.ts": [
			"eslint --fix",
			"git add"
		]
	},
	"author": "",
	"license": "GPL-3.0",
	"access": "public",
	"dependencies": {
		"@icure/motss-app-datepicker": "^6.0.0-rc.39",
		"@lit/task": "^1.0.2",
		"@webcomponents/webcomponentsjs": "^2.7.0",
